}

@article{silverstein_real_2023,
	title = {The real reason why {ATP} hydrolysis is spontaneous at {pH} {\textgreater} 7: {It}'s (mostly) the proton concentration!},
	volume = {51},
	issn = {1539-3429},
	shorttitle = {The real reason why {ATP} hydrolysis is spontaneous at {pH} {\textgreater} 7},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/bmb.21745},
	doi = {10.1002/bmb.21745},
	abstract = {Common wisdom holds that ATP hydrolysis is spontaneous because of the weakness of its phosphoanhydride bonds, electrostatic repulsion within the polyanionic ATP4− molecule, and resonance stabilization of the inorganic phosphate and ADP products. By examining the pH-dependence of the hydrolysis Gibbs free energy, we show that in fact, above pH 7, ATP hydrolysis is spontaneous due mainly to the low concentration of the H+ that is released as product. Hence, ATP is essentially just an electrophilic target whose attack by H2O causes the acidity of the water nucleophile to increase dramatically; the spontaneity of the resulting acid ionization supplies much of the released Gibbs free energy. We also find that fermentation lowers pH not due to its organic acid products (e.g., lactic, acetic, formic, or succinic acids), but again, due to the H+ product of ATP hydrolysis.},
	language = {en},
	number = {5},
	urldate = {2025-05-19},
	journal = {Biochemistry and Molecular Biology Education},
	author = {Silverstein, Todd P.},
	year = {2023},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/bmb.21745},
	keywords = {acid-base, acidity, ATP hydrolysis, biochemical thermodynamics, concentration dependence, fermentation, Gibbs free energy, pH},
	pages = {476--485},
	file = {Biochem Molecular Bio Educ - 2023 - Silverstein - The real reason why ATP hydrolysis is spontaneous at pH   7  It s  mostly.pdf:H\:\\Zotero_Library\\Biochem Molecular Bio Educ - 2023 - Silverstein - The real reason why ATP hydrolysis is spontaneous at pH   7  It s  mostly.pdf:application/pdf;Snapshot:C\:\\Users\\wai484\\Zotero\\storage\\KU8UVT55\\bmb.html:text/html},
}

@article{peluffo_nak-atpase_2023,
	title = {The {Na}+,{K}+-{ATPase} and its stoichiometric ratio: some thermodynamic speculations},
	volume = {15},
