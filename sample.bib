@article{bergman_effect_2010,
	title = {The {Effect} of {pH} and {Free} {Mg2}+ on {ATP} {Linked} {Enzymes} and the {Calculation} of {Gibbs} {Free} {Energy} of {ATP} {Hydrolysis}},
