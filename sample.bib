	author = {Ooka, Hideshi and Chiba, Yoko and Nakamura, Ryuhei},
	month = aug,
	year = {2023},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Thermodynamics, Biocatalysis, Computational models, Enzyme mechanisms},
	pages = {4860},
