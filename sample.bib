	author = {Gawthrop, Peter J.},
	month = apr,
	year = {2017},
	note = {Conference Name: IEEE Transactions on NanoBioscience},
	keywords = {Analytical models, Computational modeling, Biological system modeling, computational systems biology, systems biology, Chemicals, Context, Electric potential, Protons},
	pages = {177--188},
}

@article{gawthrop_energy-based_2022,
	title = {Energy-based advection modelling using bond graphs},
	volume = {19},
	url = {https://royalsocietypublishing.org/doi/10.1098/rsif.2022.0492},
