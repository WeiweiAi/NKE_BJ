	month = jan,
	year = {2013},
	note = {Publisher: Society for Industrial and Applied Mathematics},
	pages = {906--937},
}

@article{seslija_hamiltonian_2014,
	title = {Hamiltonian perspective on compartmental reaction–diffusion networks},
